{
	"name": "Formfill",
	"description": "Fill forms with pre-selected data.",
	"version": "0.1.0",
	"manifest_version": 3,
	"icons": {
		"16": "src/images/logo-16.png",
		"32": "src/images/logo-32.png",
		"48": "src/images/logo-48.png",
		"128": "src/images/logo-128.png",
		"150": "src/images/logo-150.png"
	},
	"options_page": "src/options/index.html",
	"permissions": [
		"storage",
		"scripting",
		"activeTab",
		"contextMenus"
	],
	"action": {
		"default_title": "Fill page forms"
	},
	"background": {
		"service_worker": "service_worker.js",
		"type": "module"
	}
}